###################################################################################
#
#   RELIC DENSITY
#
###################################################################################

#flag to use the canonical calculation or the coupled relic density calculation
  True       = relic_canonical   
#flags for calculating an array of taacs values or calculate them on the spot
  True       = calc_taacs_ann_array 
  True       = calc_taacs_dm2dm_array
  True       = calc_taacs_scattering_array

# precision of the ODE integrator which solves for relic density.
  0.01       = eps_ode

# use an approximation for the decoupling temperature in the relic density calc?
# if xd_approx = .false. use "step back" parameters [x_start,x_end,dx_step] for the ODE integrator
# (see manual for more details). Slower, but more accurate.
  False       = xd_approx

# choose whether to run the strong coupling up to the energy scale given by 2*m_chi
# if False, alpha_s is read from the param_card
  True       = running_as

###################################################################################
#
# Direct(ional) Detection 
#
###################################################################################

# choose the target material (type "help material" for more target code)
   1         = material ! 1: Xenon, 2: Germanium, 3: Silicon, ...

# galactic halo DM constants (for direct detection & capture rate by sun/earth)
   220.0     = vMP  ! The most probable WIMP velocity v_0 (km/s)
   650.0     = vescape  ! The escape velocity of WIMPS in the Halo (km/s)
   0.3       = rhoDM   ! The density of WIMPs in our local Universe (GeV/c^2/cm^-3)

# choose the size of the detector
   1000.0    = detector_size  ! For 1 ton = 1000 kg detector
   4.0       = En_threshold     ! in keVnr

# choose the energy resolution coefficient and range 
   1.0       = lambd ! lambda (sigma_E = lambda*sqrt(E))
   3.0       = sig_theta ! angular resolution
   0.0       = En_min          ! minimum recoil Energy
   100.0     = En_max          ! maximum recoil energy
   100       = Energy_bins     ! Number of Energy bins
   -1.0      = cos_min         ! Minimum cosine of recoil angle
   1.0       = cos_max         ! Maximum cosine of recoil angle
   20        = cos_theta_bins  ! Number of cosine angle bins

# running time of the detector
   0.0       = day_min         ! 1st day of observation.
   365.0     = day_max         ! 1 year later.
   10        = day_bins        ! Number of time bins

# Turn on detector smearing or not (both energy and angle)
   False     = smearing

# Nuclear form factor can be customised via parameters:
#  - scalar: SPu, SPd, SPs, SPg // SNu, SNd, SNs, SNg 
#  - vector: VPu, VPd // VNu, VNd
#  - axial vector: AVPu, AVPd, AVPs // AVNu, AVNd, AVNs
#  - Sigma(mu,nu): SigPu, SigPd, SigPs // SigNu, SigNd, SigNs

###################################################################################
#
# InDirect Detection 
#
###################################################################################

# Method of computation (affects speed and precision)
   madevent = sigmav_method ! inclusive, madevent, reshuffling
   pythia8   = indirect_flux_source_method ! pythia8, PPPC4DMID, PPPC4DMID_ew 
   dragon    =  indirect_flux_earth_method ! dragon, PPPC4DMID_ep

# setting for DM velocity
   0.001     = vave_indirect_cont  # average velocity of DM for continuum searches (default: Fermi-LAT dSph searches)
   0.00075   = vave_indirect_line  # average velocity of DM for line searches (default: GC line search analysis) 

# --------------------
# Line analysis
# --------------------

# choice of the dark matter density profile
   einasto   = profile ! gnfw, einasto, nfw, isothermal, burkert

# profile common parameters
   20.0      = r_s # kpc

# gnfw specific parameters
   1.3       = gamma

# einasto specific parameters
   0.17      = alpha

# specify a ROI for Fermi-LAT 2015
# set default to use the ROI for which your profile is optimized
   default   = roi_fermi_2015 ! default, R3, R16, R41, R90

###################################################################################
#
# GLOBAL PARAMETER
#
###################################################################################

# Flag to limit the Auto of the param_card to only consider two body decays.
   True = only_two_body_decays

# Amount of output to store in scan mode
   spectra       = save_output ! off, all, spectra

# Note that integration precision parameters can be updated via
# ngrid_init, nres_points, eps_wij, iter_wij
# those impact relic density and indirect detection (for sigmav_method=inclusive)

